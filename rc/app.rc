            VALUE "OriginalFilename", "DemoCleaner3.exe"
            VALUE "ProductName",      "DemoCleaner3"
            VALUE "ProductVersion",   "3.3.0.1"
        END
    END

    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1250
    END
